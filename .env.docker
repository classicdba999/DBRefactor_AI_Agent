# Docker Environment Configuration
# Copy this file to .env and update with your values

# Source Database (Oracle) - External
SOURCE_DB_HOST=your-oracle-host
SOURCE_DB_PORT=1521
SOURCE_DB_NAME=ORCL
SOURCE_DB_USER=your_oracle_user
SOURCE_DB_PASSWORD=your_oracle_password

# Target Database (PostgreSQL) - Uses Docker service
TARGET_DB_HOST=postgres
TARGET_DB_PORT=5432
TARGET_DB_NAME=postgres
TARGET_DB_USER=postgres
TARGET_DB_PASSWORD=postgres

# Application Database (MySQL) - Uses Docker service
APP_DB_HOST=mysql
APP_DB_PORT=3306
APP_DB_NAME=migration_tracker
APP_DB_USER=root
APP_DB_PASSWORD=mysql

# AI API Keys
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Redis - Uses Docker service
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# Application
DEBUG=False
ENVIRONMENT=production
LOG_LEVEL=INFO
